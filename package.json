{
  "name": "guess-the-number-ui",
  "version": "1.0.0",
  "description": "Guess The Number UI",
  "license": "Apache-2.0",
  "main": "src/index.tsx",
  "scripts": {
    "clean": "rm -rf dist .papi",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "gen-pah-rpc": "yarn papi add -w wss://testnet-passet-hub.polkadot.io pah",
    "gen-ink-v6-contract-metadata": "yarn papi ink add metadata/guess_the_number.json",
    "generate": "yarn gen-ink-v6-contract-metadata && yarn gen-pah-rpc",
    "postinstall": "yarn generate"
  },
  "peerDependencies": {
    "polkadot-api": "*"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@polkadot-api/sdk-ink": "^0.4.3",
    "@polkadot/keyring": "^13.5.6",
    "@reactive-dot/react": "^0.46.2",
    "dayjs": "^1.11.18",
    "dot-connect": "^0.22.1",
    "polkadot-api": "^1.18.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-hot-toast": "^2.6.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "latest",
    "vite": "latest"
  }
}
